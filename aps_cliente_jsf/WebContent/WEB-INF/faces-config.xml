<?xml version="1.0" encoding="UTF-8"?>
<faces-config version="2.2" xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
	<navigation-rule>
		<from-view-id>*</from-view-id>
		<navigation-case>
			<from-outcome>novoCliente</from-outcome>
			<to-view-id>/faces/cliente/manterCliente.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>*</from-view-id>
		<navigation-case>
			<from-outcome>editarCliente</from-outcome>
			<to-view-id>/faces/cliente/manterCliente.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>*</from-view-id>
		<navigation-case>
			<from-outcome>listarClientes</from-outcome>
			<to-view-id>/faces/cliente/listar/listarCliente.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>*</from-view-id>
		<navigation-case>
			<from-outcome>novoOrcamento</from-outcome>
			<to-view-id>/faces/orcamento/manterOrcamento.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>*</from-view-id>
		<navigation-case>
			<from-outcome>selecionarClienteOrcamento</from-outcome>
			<to-view-id>/faces/cliente/listar/dialog/dialogListarCliente.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>*</from-view-id>
		<navigation-case>
			<from-outcome>novoProduto</from-outcome>
			<to-view-id>/faces/produto/manterProduto.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>*</from-view-id>
		<navigation-case>
			<from-outcome>editarProduto</from-outcome>
			<to-view-id>/faces/produto/manterProduto.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>*</from-view-id>
		<navigation-case>
			<from-outcome>listarProdutos</from-outcome>
			<to-view-id>/faces/produto/listar/listarProduto.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<application>
		<locale-config>
			<default-locale>pt_BR</default-locale>
		</locale-config>
		<resource-bundle>
			<base-name>bundle.mensagens</base-name>
			<var>msg</var>
		</resource-bundle>
		<message-bundle>bundle.mensagens</message-bundle>
	</application>
	<factory>
		<exception-handler-factory>br.com.aps.cliente.jsf.exceptions.CustomExceptionHandlerFactory</exception-handler-factory>
	</factory>
</faces-config>
